{
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "noEmit": true,
    "outDir": "dist",
    "skipLibCheck": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules/", "dist/"]
}